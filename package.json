{
  "name": "storj-cloud-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && electron-builder install-app-deps",
    "start": "nx serve",
    "start:desktop": "npm run serve:desktop",
    "build": "nx build",
    "test": "nx test",
    "serve:desktop": "electron ./dist/apps/storj-cloud-desktop/main.js",
    "build:desktop": "rm -rf ./dist/apps && nx build storj-cloud-browser build --base-href ./ &&  nx build storj-cloud-desktop",
    "make:browser": "nx build storj-cloud-browser build --base-href ./",
    "make:desktop": "electron-builder --config=electron-builder.yml",
    "deploy:browser": "npx gh-pages -d dist/apps/storj-cloud-browser",
    "deploy:desktop": "electron-builder --config=electron-builder.yml -- --publish always"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ionic/angular": "^6.0.15",
    "@nrwl/angular": "13.9.7",
    "@nrwl/node": "^13.10.1",
    "aws-sdk": "^2.1109.0",
    "core-js": "^3.6.5",
    "electron-squirrel-startup": "^1.0.0",
    "regenerator-runtime": "0.13.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@nrwl/cli": "13.10.0",
    "@nrwl/cypress": "13.10.0",
    "@nrwl/eslint-plugin-nx": "13.10.0",
    "@nrwl/jest": "13.10.0",
    "@nrwl/linter": "13.10.0",
    "@nrwl/web": "13.10.1",
    "@nrwl/workspace": "13.10.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cypress": "^9.1.0",
    "electron": "^18.0.3",
    "electron-builder": "^23.0.3",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
